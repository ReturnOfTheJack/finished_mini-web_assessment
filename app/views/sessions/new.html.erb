<h1>Course Content Manager</h1>
<% flash.each do |message_type, message| %>
	<div><%= message %></div>
<% end %>

<div>
	<div id=login_field>
		<%= form_for :session, url: login_path do |f| %>
		<%= f.label :email %>
		<%= f.email_field :email %>

		<%= f.label :password %>
		<%= f.password_field :password %>
	</div>

	<div id=logbutton>
		<%= f.submit "Log in" %>
	</div>
	
	<% end %>
</div>
